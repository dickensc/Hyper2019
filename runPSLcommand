java -Xmx370G -Xms370G -jar ./psl-cli-2.2.1.jar --model ../lastfm-quadratic/cli/lastfm.psl --data lastfm-learn.data --learn org.linqs.psl.application.learning.weight.maxlikelihood.MaxPiecewisePseudoLikelihood -int-ids --postgres psl -D log4j.threshold=TRACE persistedatommanager.throwaccessexception=false -D votedperceptron.stepsize=10 -D votedperceptron.numsteps=100
java -Xmx370G -Xms370G -jar ./psl-cli-2.2.1.jar --infer --eval org.linqs.psl.evaluation.statistics.ContinuousEvaluator --output inferred-predicates --model ../lastfm-linear/cli/lastfm-learned.psl --data lastfm-eval.data -int-ids --postgres psl -D log4j.threshold=TRACE persistedatommanager.throwaccessexception=false